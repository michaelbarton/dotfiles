snippet help
	require 'spec_helper'${1}
snippet desc
	describe "${1:description}" do
		${2}
	end
snippet bef
	before(:${1:each}) do
		${2}
	end
snippet aft
	after(:${1:each}) do
		${2}
	end
snippet it
	it "should ${1:description}" do
		${2}
	end
snippet let
	let(:${1:description}) do
		${2}
	end
snippet its
	its(${1:attribute}){ should ${2} }
snippet subj
	subject do
		${1}
	end
snippet sheq
	${1:target}.should == ${2}
snippet shbe
	${1:target}.should be_${2:result}
